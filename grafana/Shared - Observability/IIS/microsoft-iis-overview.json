          {
            "datasource": {
              "uid": "${prometheus_datasource}"
            },
            "expr": "rate(windows_iis_requests_total{job=~\"$job\", instance=~\"$instance\", site=~\"$site\"}[$__rate_interval])",
            "format": "time_series",
            "intervalFactor": 2,
            "legendFormat": "{{site}} - {{method}}",
            "refId": "A"
